# Stripe Configuration
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# AWS Configuration
VITE_AWS_REGION=ap-southeast-1
VITE_USER_POOL_ID=your_cognito_user_pool_id
VITE_USER_POOL_CLIENT_ID=your_cognito_client_id
VITE_API_ENDPOINT=https://your-api-id.execute-api.ap-southeast-1.amazonaws.com/prod
VITE_WEBSOCKET_URL=wss://your-websocket-api-id.execute-api.ap-southeast-1.amazonaws.com/prod/

# Admin Insights WebSocket Configuration
# This is the WebSocket endpoint for the Admin Insights Agent
# Format: wss://{api-id}.execute-api.{region}.amazonaws.com/{stage}
# Get this value from CDK output: E-Com67-AdminInsightsWebSocketURL
VITE_ADMIN_INSIGHTS_WEBSOCKET_URL=wss://your-admin-insights-websocket-api-id.execute-api.ap-southeast-1.amazonaws.com/prod

# Development Configuration
VITE_ENV=development
